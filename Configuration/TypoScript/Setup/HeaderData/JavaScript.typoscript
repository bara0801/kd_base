[globalVar = LIT:1 = {$config.bootstrap.enable} && globalVar = LIT:0 = {$config.bootstrap.local}]
page.includeJSFooterlibs{
    bootstrap = {$config.bootstrap.js.cdnUrl}
    bootstrap.external = 1
}
[global]
[globalVar = LIT:1 = {$config.bootstrap.enable}] && [globalVar = LIT:1 = {$config.bootstrap.cdn}] && [globalVar = LIT:0 = {$config.bootstrap.local}]
page.includeJSFooterlibs{
    bootstrap.ifEmpty = //maxcdn.bootstrapcdn.com/bootstrap/{$config.bootstrap.version}/js/bootstrap.min.js
    bootstrap.external = 1
}
[global]
[globalVar = LIT:1 = {$config.bootstrap.enable}] && [globalVar = LIT:1 = {$config.bootstrap.local}]
page.includeJSFooterlibs{
    bootstrap = {$config.bootstrap.localPath}/bootstrap-{$config.bootstrap.version}.min.js
    bootstrap.external = 0
    bootstrap.ifEmpty = EXT:kd_base/Resources/Public/JavaScript/Bootstrap/bootstrap-{$config.bootstrap.version}.min.js
}  
[global]
page.includeJS{
    template = EXT:kd_base/Resources/Public/JavaScript/template.js
}
